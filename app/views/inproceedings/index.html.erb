<p id="notice"><%= notice %></p>

<h1>Inproceedings</h1>

<table>
  <thead>
    <tr>
      <th>Key</th>
      <th>Author</th>
      <th>Title</th>
      <th>Booktitle</th>
      <th>Year</th>
      <th>Editor</th>
      <th>Volume</th>
      <th>Series</th>
      <th>Pages</th>
      <th>Address</th>
      <th>Month</th>
      <th>Organization</th>
      <th>Publisher</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inproceedings.each do |inproceeding| %>
      <tr>
        <td><%= inproceeding.key %></td>
        <td><%= inproceeding.author %></td>
        <td><%= inproceeding.title %></td>
        <td><%= inproceeding.booktitle %></td>
        <td><%= inproceeding.year %></td>
        <td><%= inproceeding.editor %></td>
        <td><%= inproceeding.volume %></td>
        <td><%= inproceeding.series %></td>
        <td><%= inproceeding.pages %></td>
        <td><%= inproceeding.address %></td>
        <td><%= inproceeding.month %></td>
        <td><%= inproceeding.organization %></td>
        <td><%= inproceeding.publisher %></td>
        <td><%= inproceeding.note %></td>
        <td class="buttons"><%= link_to 'Download bibtex', inproc_download_path(inproceeding.id), class: "button secondary" %>
        <%= link_to 'Show', inproceeding, class: "button secondary" %>
        <%= link_to 'Edit', edit_inproceeding_path(inproceeding), class: "button secondary" %>
        <%= link_to 'Destroy', inproceeding, method: :delete, data: { confirm: 'Are you sure?' }, class: "button secondary" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inproceeding', new_inproceeding_path, class: "button secondary" %>
